{% extends 'base.html.twig' %}

{% block title %}Hello EtablissementController!{% endblock %}

{% block body %}
    <style>
        .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
        .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
    </style>

    <div class="example-wrapper">
        <p>{{ etablissement.titre }}</p>
        <h1>{{ etablissement.nom }}</h1>
        <h2>{{ etablissement.ville }}</h2>
        <h3>{{ etablissement.codePostal }}</h3>
        <p>{{ etablissement.description }}</p>

    </div>

    {{ block('body', 'suite/suites.html.twig') }}
{% endblock %}
